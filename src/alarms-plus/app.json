{
  "expo": {
    "name": "Alarms Plus",
    "displayName": "Alarms Plus",
    "slug": "alarms-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "splash": {
      "image": "./images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },    
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@notifee/react-native"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geeoon.alarmsplus",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/icon.png",
        "monochromeImage": "./images/mono-icon.png",
        "backgroundColor": "#ffffff" 
      },
      "package": "com.geeoon.alarmsplus",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "325d1a69-22c1-4683-a94b-25ae6243d10d"
      }
    },
    "owner": "geeoonuw"
  },
  "name": "alarms-plus"
}
